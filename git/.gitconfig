[user]
	name = Juan Sanchez
	email = 14128083+dexter0323@users.noreply.github.com
[core]
	pager = cat
	editor = code
[pager]
	diff = less -FX
[init]
	defaultBranch = master
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
[alias]
	co = checkout
	ci = commit -m
	st = status
	br = branch
	reset1 = reset HEAD~1
	pushfwl = push --force-with-lease
	mergemaster = pull origin master --no-edit
	rebasemaster = pull --rebase origin master
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	log = log --graph --decorate --oneline --all
	type = cat-file -t
	dump = cat-file -p
[pull]
	rebase = false
[commit]
	gpgsign = true
